            </div>
            <div class="mb-3">
                <input type="number" id="num2" class="form-control" placeholder="Enter second number">
            </div>
            <button class="btn btn-primary w-100 mb-3 btn-custom" onclick="calculate()">Calculate</button>
            <div id="result" class="alert alert-info" role="alert"></div>
        </div>
    </div>

    <!-- Bootstrap JS Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>

    <script>
        function calculate() {
            const num1 = parseFloat(document.getElementById('num1').value);
            const num2 = parseFloat(document.getElementById('num2').value);
            let result = '';

            if (!isNaN(num1) && !isNaN(num2)) {
                const addition = num1 + num2;
                const subtraction = num1 - num2;
                const multiplication = num1 * num2;
                const division = num2 !== 0 ? num1 / num2 : 'Cannot divide by zero';
                const average = (num1 + num2) / 2;

                result = `<strong>Addition:</strong> ${addition} <br>
                          <strong>Subtraction:</strong> ${subtraction} <br>
                          <strong>Multiplication:</strong> ${multiplication} <br>
                          <strong>Division (Quotient):</strong> ${division} <br>
                          <strong>Average:</strong> ${average}`;
            } else {
                result = 'Please enter valid numbers.';
            }

            document.getElementById('result').innerHTML = result;
        }
    </script>
